{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Principal": {
        "AWS": "*"
      },
      "Action": [
        "s3:GetObject"
      ],
      "Resource": "arn:aws:s3:::ml-models/*"
    },
    {
      "Effect": "Allow",
      "Principal": {
        "AWS": "*"
      },
      "Action": [
        "s3:ListBucket"
      ],
      "Resource": "arn:aws:s3:::ml-models"
    },
    {
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam:::user/ml-service"
      },
      "Action": [
        "s3:PutObject",
        "s3:DeleteObject",
        "s3:PutObjectVersion"
      ],
      "Resource": "arn:aws:s3:::ml-models/*"
    }
  ]
}